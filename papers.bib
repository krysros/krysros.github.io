@article{aip2023,
  title        = {Solution of thin rectangular plates with various boundary conditions},
  author       = {Delyavskyy, Mykhaylo and Sobczak-Piąstka, Justyna and Rosinski, Krystian and Buchaniec, Dariusz and Famulyak, Yuriy},
  year         = 2023,
  month        = {08},
  journal      = {AIP Conference Proceedings},
  volume       = 2949,
  number       = 1,
  pages        = {020023},
  doi          = {10.1063/5.0165300},
  issn         = {0094-243X},
  url          = {https://doi.org/10.1063/5.0165300},
  abstract     = {The paper presents an analytical-numerical method to solve thin rectangular plate constructions loaded by continuous load. The developed technique is based on the model of continuum material and allows to reduce the structure modeling to a solution that consists of kinematic and static boundary conditions written in the separate points at the plate contour. This has led to obtaining elegant and compact analytic solution capable of easy numerical evaluation. The analyzed problem is an extension of the description presented in the paper [1]. In contrast to FEM, the suggested technique uses essentially fewer equations to solve the structure and provides better accuracy of solution. To solve a problem, the FEM subdivides structure into many smaller parts that are called finite elements, while suggested technique uses large separate parts called macroelements [2, 3].},
  language     = {english},
  hyphenation  = {english}
}
